   "source": [
    "from arm import BernoulliArm\n",
    "from greedy import Greedy\n",
    "\n",
    "mus = [0.1, 0.6, 0.8, 0.2]\n",
    "arms = [BernoulliArm(mu) for mu in mus]\n",
    "N=1000\n",
    "alg = Greedy(arms,N)\n",
    "print(arms[2])\n",
    "print(alg)"
   ]
  },
